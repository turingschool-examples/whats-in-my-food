<h3>Total Items: <%= @total_hits %></h3>

<h3>Top Ten Foods: </h3>

<% if @top_ten.count > 0 %>
  <% @top_ten.each do |food| %>
    <p>GTIN/UPC Code: <%= food.upc %></p>
    <p>Description: <%= food.description %></p>
    <p>Brand Owner: <%= food.brand_owner %></p>
    <p>Ingredients: <%= food.ingredients %></p><br>
  <% end %>
<% else %>
  <p>No results</p>
<% end %>
