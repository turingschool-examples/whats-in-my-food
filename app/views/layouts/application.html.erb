<!DOCTYPE html>
<html>
  <head>
    <title>WhatsInMyFood</title>
    <%= csrf_meta_tags %>

    <%= stylesheet_link_tag    'application', media: 'all' %>
    <%= javascript_include_tag 'application' %>
  </head>

  <body>
    <% flash.each do |type, msg| %>
      <div class="flash-notice">
        <%= msg %>
      </div>
    <% end %>
    
    <%= yield %>
  </body>

</html>
